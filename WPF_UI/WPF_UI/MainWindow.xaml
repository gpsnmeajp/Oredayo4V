			<TabItem  Header="{DynamicResource Tab_ModelSetting}">
				<ScrollViewer>
					<StackPanel>
						<GroupBox Header="{DynamicResource Tab_ModelSetting}" BorderThickness="0" materialDesign:ColorZoneAssist.Mode="PrimaryDark">
							<StackPanel>
								<materialDesign:Card Margin="5,5,5,5" >
