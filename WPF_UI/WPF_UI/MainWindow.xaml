										<DockPanel Margin="5,5,5,5">
											<DockPanel>
												<Grid>
													<Grid.ColumnDefinitions>
														<ColumnDefinition/>
														<ColumnDefinition/>
													</Grid.ColumnDefinitions>
													<Grid.RowDefinitions>
														<RowDefinition />
														<RowDefinition />
													</Grid.RowDefinitions>
													<Button Grid.Column="0" Grid.Row="0"  Style="{StaticResource MaterialDesignRaisedAccentButton}"  Foreground="#444444" Margin="5,5,5,5" Content="{DynamicResource Tab_Welcome_Preset1}" Click="Preset1_Cliecked"/>
													<Button Grid.Column="1" Grid.Row="0"  Style="{StaticResource MaterialDesignRaisedAccentButton}"  Foreground="#444444" Margin="5,5,5,5" Content="{DynamicResource Tab_Welcome_Preset2}" Click="Preset2_Cliecked" />
													<Button Grid.Column="0" Grid.Row="1"  Style="{StaticResource MaterialDesignRaisedAccentButton}"  Foreground="#444444" Margin="5,5,5,5" Content="{DynamicResource Tab_Welcome_Preset3}" Click="Preset3_Cliecked" />
													<Button Grid.Column="1" Grid.Row="1"  Style="{StaticResource MaterialDesignRaisedAccentButton}"  Foreground="#444444" Margin="5,5,5,5" Content="{DynamicResource Tab_Welcome_Preset4}" Click="Preset4_Cliecked" />
												</Grid>
											</DockPanel>
										</DockPanel>
									</Expander>
								</materialDesign:Card>
								<materialDesign:Card Margin="5,5,5,5" >
									<DockPanel Margin="5,5,5,5">
										<TextBlock DockPanel.Dock="Top" Style="{StaticResource MaterialDesignSubtitle1TextBlock}" Foreground="#333333" Margin="10,10,10,10" Text="{DynamicResource Tab_Welcome_SNS}"></TextBlock>
										<DockPanel>
											<Grid>
												<DockPanel>
													<StackPanel  DockPanel.Dock="Left">
														<TextBlock Foreground="#333333" Margin="10,10,10,10"><Hyperlink NavigateUri="https://twitter.com/intent/tweet?hashtags=Oredayo4V" RequestNavigate="Hyperlink_RequestNavigate">Tweet</Hyperlink></TextBlock>
														<TextBlock Foreground="#333333" Margin="10,10,10,10"><Hyperlink NavigateUri="https://twitter.com/search?q=%23Oredayo4V" RequestNavigate="Hyperlink_RequestNavigate">HashTag #Oredayo4V</Hyperlink></TextBlock>
														<TextBlock Foreground="#333333" Margin="10,10,10,10"><Hyperlink NavigateUri="https://discord.com/invite/nGapSR7" RequestNavigate="Hyperlink_RequestNavigate">Support Discord</Hyperlink></TextBlock>
														<TextBlock Foreground="#333333" Margin="10,10,10,10" Text="{DynamicResource Tab_Welcome_SNS_DiscordHelp}"/>
													</StackPanel>
													<Image DockPanel.Dock="Right" HorizontalAlignment="Right" Margin="10,10,10,10" Source="Resources/vmpc_logo_128x128.png" MaxWidth="128" />
												</DockPanel>
											</Grid>
										</DockPanel>
									</DockPanel>
								</materialDesign:Card>
							</StackPanel>
						</GroupBox>
					</StackPanel>
				</ScrollViewer>
			</TabItem>
			<TabItem Name="TabBasicSetting" Header="{DynamicResource Tab_BasicSetting}">
